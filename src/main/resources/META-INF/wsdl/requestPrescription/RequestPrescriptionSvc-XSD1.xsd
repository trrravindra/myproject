<?xml version="1.0" encoding="utf-8"?>
<xs:schema elementFormDefault="qualified"
	targetNamespace="http://schemas.datacontract.org/2004/07/RxAssystLib.Contracts.Data"
	xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:tns="http://schemas.datacontract.org/2004/07/RxAssystLib.Contracts.Data">
	<xs:import
		schemaLocation="RequestPrescriptionSvc-XSD2.xsd"
		namespace="http://schemas.datacontract.org/2004/07/RxAssystLib.Codes" />
	<xs:complexType name="UserToken">
		<xs:sequence>
			<xs:element minOccurs="0" name="Status" type="q1:eTokenStatus"
				xmlns:q1="http://schemas.datacontract.org/2004/07/RxAssystLib.Codes" />
			<xs:element minOccurs="0" name="Token" nillable="true"
				type="xs:base64Binary" />
		</xs:sequence>
	</xs:complexType>
	<xs:element name="UserToken" nillable="true" type="tns:UserToken" />
	<xs:complexType name="RxNewRequest">
		<xs:complexContent mixed="false">
			<xs:extension base="tns:Request">
				<xs:sequence>
					<xs:element name="PatientDefaultPharmacy" type="xs:short" />
					<xs:element name="Prescriptions" nillable="true"
						type="tns:RxRenewPrescriptions" />
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="RxNewRequest" nillable="true" type="tns:RxNewRequest" />
	<xs:complexType name="Request">
		<xs:sequence>
			<xs:element name="Comments" nillable="true" type="xs:string" />
			<xs:element name="Delivery" nillable="true" type="tns:RequestDelivery" />
			<xs:element name="EMail" nillable="true" type="xs:string" />
			<xs:element name="LastState" type="q2:eRequestState"
				xmlns:q2="http://schemas.datacontract.org/2004/07/RxAssystLib.Codes" />
			<xs:element name="LastStateDate" type="xs:dateTime" />
			<xs:element name="MedReleaseMode" type="q3:eMedReleaseMode"
				xmlns:q3="http://schemas.datacontract.org/2004/07/RxAssystLib.Codes" />
			<xs:element minOccurs="0" name="OID" nillable="true" type="xs:string" />
			<xs:element name="PharmacyContractNumber" nillable="true"
				type="xs:string" />
			<xs:element name="ReleaseDate" type="xs:dateTime" />
			<xs:element minOccurs="0" name="Phone" nillable="true"
				type="xs:string" />
		</xs:sequence>
	</xs:complexType>
	<xs:element name="Request" nillable="true" type="tns:Request" />
	<xs:complexType name="RequestDelivery">
		<xs:sequence>
			<xs:element name="Address" nillable="true" type="xs:string" />
			<xs:element name="City" nillable="true" type="xs:string" />
			<xs:element name="Fax" nillable="true" type="xs:string" />
			<xs:element minOccurs="0" name="Phone" nillable="true"
				type="xs:string" />
			<xs:element name="PostalCode" nillable="true" type="xs:string" />
			<xs:element name="Province" type="q4:eProvince"
				xmlns:q4="http://schemas.datacontract.org/2004/07/RxAssystLib.Codes" />
		</xs:sequence>
	</xs:complexType>
	<xs:element name="RequestDelivery" nillable="true"
		type="tns:RequestDelivery" />
	<xs:complexType name="RxRenewPrescriptions">
		<xs:sequence>
			<xs:element minOccurs="0" maxOccurs="unbounded" name="RxRenewPrescription"
				nillable="true" type="tns:RxRenewPrescription" />
		</xs:sequence>
	</xs:complexType>
	<xs:element name="RxRenewPrescriptions" nillable="true"
		type="tns:RxRenewPrescriptions" />
	<xs:complexType name="RxRenewPrescription">
		<xs:sequence>
			<xs:element name="Comments" nillable="true" type="xs:string" />
			<xs:element name="OID" nillable="true" type="xs:string" />
		</xs:sequence>
	</xs:complexType>
	<xs:element name="RxRenewPrescription" nillable="true"
		type="tns:RxRenewPrescription" />
	<xs:complexType name="RxRequest">
		<xs:complexContent mixed="false">
			<xs:extension base="tns:Request">
				<xs:sequence>
					<xs:element name="Patient" nillable="true" type="tns:RxPatient" />
					<xs:element name="Prescriptions" nillable="true"
						type="tns:RxPrescriptions" />
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="RxRequest" nillable="true" type="tns:RxRequest" />
	<xs:complexType name="RxPatient">
		<xs:sequence>
			<xs:element name="Address" nillable="true" type="xs:string" />
			<xs:element minOccurs="0" name="BirthDate" type="xs:int" />
			<xs:element name="City" nillable="true" type="xs:string" />
			<xs:element name="FirstName" nillable="true" type="xs:string" />
			<xs:element name="Gender" type="q5:eGender"
				xmlns:q5="http://schemas.datacontract.org/2004/07/RxAssystLib.Codes" />
			<xs:element name="Language" type="q6:eLanguage"
				xmlns:q6="http://schemas.datacontract.org/2004/07/RxAssystLib.Codes" />
