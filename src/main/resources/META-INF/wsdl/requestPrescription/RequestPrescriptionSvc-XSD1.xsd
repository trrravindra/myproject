			<xs:element name="OID" nillable="true" type="xs:string" />
			<xs:element name="PHN" nillable="true" type="xs:string" />
			<xs:element name="PatientFile" nillable="true" type="xs:string" />
			<xs:element name="PhoneNumber" nillable="true" type="xs:string" />
			<xs:element name="PostalCode" nillable="true" type="xs:string" />
			<xs:element name="Province" type="q7:eProvince"
				xmlns:q7="http://schemas.datacontract.org/2004/07/RxAssystLib.Codes" />
			<xs:element minOccurs="0" name="BirthDateV2" nillable="true"
				type="xs:dateTime">
				<xs:annotation>
					<xs:appinfo>
						<DefaultValue EmitDefaultValue="false"
							xmlns="http://schemas.microsoft.com/2003/10/Serialization/" />
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="RxPatient" nillable="true" type="tns:RxPatient" />
	<xs:complexType name="RxPrescriptions">
		<xs:sequence>
			<xs:element minOccurs="0" maxOccurs="unbounded" name="RxPrescription"
				nillable="true" type="tns:RxPrescription" />
		</xs:sequence>
	</xs:complexType>
	<xs:element name="RxPrescriptions" nillable="true"
		type="tns:RxPrescriptions" />
	<xs:complexType name="RxPrescription">
		<xs:sequence>
			<xs:element name="Aborted" type="xs:boolean" />
			<xs:element name="Comments" nillable="true" type="xs:string" />
			<xs:element name="DaysSupply" type="xs:short" />
			<xs:element minOccurs="0" name="ExpirationDate" type="xs:int" />
			<xs:element name="LastState" type="q8:eRxPrescriptionState"
				xmlns:q8="http://schemas.datacontract.org/2004/07/RxAssystLib.Codes" />
			<xs:element name="LastStateDate" type="xs:dateTime" />
			<xs:element name="Narcotic" type="xs:boolean" />
			<xs:element name="OID" nillable="true" type="xs:string" />
			<xs:element name="PRN" type="xs:boolean" />
			<xs:element name="ProductDin" nillable="true" type="xs:string" />
			<xs:element minOccurs="0" name="ProductForm" nillable="true"
				type="xs:string" />
			<xs:element name="ProductName" nillable="true" type="xs:string" />
			<xs:element minOccurs="0" name="ProductStrength" nillable="true"
				type="xs:string" />
			<xs:element name="QuantityFilled" type="xs:int" />
			<xs:element name="QuantityPrescribed" type="xs:int" />
			<xs:element name="Repeats" type="xs:short" />
			<xs:element minOccurs="0" name="RxDate" type="xs:int" />
			<xs:element name="RxNewPrescription" nillable="true"
				type="tns:RxPrescription" />
			<xs:element name="RxNumber" nillable="true" type="xs:string" />
			<xs:element minOccurs="0" name="SigDecoded" nillable="true"
				type="xs:string" />
			<xs:element name="Total" type="xs:int" />
			<xs:element name="TrxNumber" type="xs:long" />
			<xs:element minOccurs="0" name="RxDateV2" type="xs:dateTime">
				<xs:annotation>
					<xs:appinfo>
						<DefaultValue EmitDefaultValue="false"
							xmlns="http://schemas.microsoft.com/2003/10/Serialization/" />
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs="0" name="ExpirationDateV2"
				nillable="true" type="xs:dateTime">
				<xs:annotation>
					<xs:appinfo>
						<DefaultValue EmitDefaultValue="false"
							xmlns="http://schemas.microsoft.com/2003/10/Serialization/" />
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs="0" name="Restrictions" nillable="true"
				type="tns:PrescriptionRestrictions" />
		</xs:sequence>
	</xs:complexType>
	<xs:element name="RxPrescription" nillable="true"
		type="tns:RxPrescription" />
	<xs:complexType name="PrescriptionRestrictions">
		<xs:sequence>
			<xs:element minOccurs="0" maxOccurs="unbounded"
				name="PrescriptionRestriction" nillable="true"
				type="tns:PrescriptionRestriction" />
		</xs:sequence>
	</xs:complexType>
	<xs:element name="PrescriptionRestrictions" nillable="true"
		type="tns:PrescriptionRestrictions" />
	<xs:complexType name="PrescriptionRestriction">
		<xs:sequence>
			<xs:element name="Type" type="q9:eRestriction"
				xmlns:q9="http://schemas.datacontract.org/2004/07/RxAssystLib.Codes" />
			<xs:element name="RefillAllowed" type="xs:boolean" />
			<xs:element name="RequireETA" type="xs:boolean" />
		</xs:sequence>
	</xs:complexType>
	<xs:element name="PrescriptionRestriction" nillable="true"
		type="tns:PrescriptionRestriction" />
	<xs:complexType name="RequestStateWrapper">
		<xs:sequence>
			<xs:element name="State" type="q10:eRequestState"
				xmlns:q10="http://schemas.datacontract.org/2004/07/RxAssystLib.Codes" />
		</xs:sequence>
	</xs:complexType>
	<xs:element name="RequestStateWrapper" nillable="true"
		type="tns:RequestStateWrapper" />
	<xs:complexType name="Paging">
		<xs:sequence>
			<xs:element name="PageSize" type="xs:int" />
			<xs:element name="Start" type="xs:int" />
		</xs:sequence>
	</xs:complexType>
	<xs:element name="Paging" nillable="true" type="tns:Paging" />
	<xs:complexType name="RxListRequests">
		<xs:sequence>
			<xs:element minOccurs="0" maxOccurs="unbounded" name="RxListRequest"
				nillable="true" type="tns:RxListRequest" />
		</xs:sequence>
	</xs:complexType>
	<xs:element name="RxListRequests" nillable="true"
		type="tns:RxListRequests" />
