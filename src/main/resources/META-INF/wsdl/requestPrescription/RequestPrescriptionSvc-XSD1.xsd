		<xs:complexContent mixed="false">
			<xs:extension base="tns:Request">
				<xs:sequence>
					<xs:element name="Prescriptions" nillable="true"
						type="tns:RxPrescriptions" />
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="RxListRequest" nillable="true"
		type="tns:RxListRequest" />
	<xs:complexType name="RequestUnsubscribe">
		<xs:sequence>
			<xs:element name="Patient" nillable="true" type="tns:RxPatient" />
			<xs:element name="Reason" type="q11:eUnsubscribeReason"
				xmlns:q11="http://schemas.datacontract.org/2004/07/RxAssystLib.Codes" />
			<xs:element name="ReasonComment" nillable="true" type="xs:string" />
			<xs:element name="ProfileReplication" type="q12:eProfileReplicationOperation"
				xmlns:q12="http://schemas.datacontract.org/2004/07/RxAssystLib.Codes" />
			<xs:element name="State" type="q13:eUnsubscribeRequestState"
				xmlns:q13="http://schemas.datacontract.org/2004/07/RxAssystLib.Codes" />
			<xs:element name="LastStateDate" type="xs:dateTime" />
			<xs:element name="OID" nillable="true" type="xs:string" />
		</xs:sequence>
	</xs:complexType>
	<xs:element name="RequestUnsubscribe" nillable="true"
		type="tns:RequestUnsubscribe" />
</xs:schema>